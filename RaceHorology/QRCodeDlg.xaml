<Window x:Class="RaceHorology.QRCodeDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RaceHorology"
        mc:Ignorable="d"
        Title="QR Code für Sprecheransicht" Height="450" Width="800">
    <Grid>
    <Image x:Name="imgQRCode" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Top" Stretch="Uniform" PreviewMouseDown="onClickQRCode"/>
    </Grid>
</Window>
